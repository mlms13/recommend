{
  "name": "recommend",
  "bsc-flags": [
    "-bs-no-version-header",
    "-bs-super-errors"
  ],
  "reason": {
    "react-jsx": 2
  },
  "bs-dependencies": [
    "bs-abstract",
    "bs-nonempty",
    "reason-react",
    "reason-future",

    // dev, belong in "examples" bsconfig
    "bs-fetch",
    "re-yaml",
    "bs-decode"
  ],
  "bs-dev-dependencies": [
  ],
  "sources": [
    {
      "dir": "./src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "refmt": 3,
  "suffix": ".bs.js",
  "warnings": {
    "error": "+8+11"
  }
}
